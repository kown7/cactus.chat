/*
  Strict-Transport-Security: max-age=31536000; includeSubDomains; preload
  X-Content-Type-Options: nosniff
  X-XSS-Protection: 1; mode=block
  Content-Security-Policy: default-src 'self'; frame-ancestors https://jamstackthemes.dev; manifest-src 'self'; connect-src *; font-src 'self'; img-src * data:; script-src 'self' https://*.cactus.chat 'unsafe-inline'; style-src 'self' https://*.cactus.chat 'unsafe-inline'; frame-src 'self' https://www.youtube.com/embed/
  X-Frame-Options: SAMEORIGIN
  Referrer-Policy: strict-origin
  Feature-Policy: geolocation 'self'
  Cache-Control: public, max-age=3600
